{
    "manifest_version": 2,
    "name": "TabSpaces",
    "author": "Sol Zilberman",
    "version": "1.0.0",
    "description": "Replace new tab screen with TabSpaces.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "favicon-16.png",
        "48": "favicon-48.png",
        "128": "128.png"
    }
}